mysql> drop table if exists test.t
mysql> create table test.t (a datetime(6))
mysql> insert into test.t values('2021-03-13 12:34:56.123456')
mysql> alter table test.t set tiflash replica 1
func> wait_table test t

mysql> set session tidb_isolation_read_engines='tiflash'

mysql> select a from test.t where extract(year from a) = 2021 and extract(month from a) = 3 and extract(week from a) = 10 and extract(day from a) = 13;
a
2021-03-13 12:34:56.123456

mysql> select a from test.t where extract(day_microsecond from a) = 13123456123456 and extract(day_second from a) = 13123456 and extract(day_minute from a) = 131234 and extract(day_hour from a) = 1312 and extract(year_month from a) = 202103;
a
2021-03-13 12:34:56.123456

mysql> drop table if exists test.t
