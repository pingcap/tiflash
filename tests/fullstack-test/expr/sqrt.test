|           1.1 |           1.2 |          0.5 |            2 |
+---------------+---------------+--------------+--------------+

mysql> set @@tidb_enforce_mpp=on; select /*+ READ_FROM_STORAGE(TIFLASH[t]) */ sqrt(f * -1.0) as v from test.t;
