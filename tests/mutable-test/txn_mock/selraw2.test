=> drop table if exists default.test
=> DBGInvoke __drop_tidb_table(default, test)

=> DBGInvoke __set_flush_threshold(100000, 0)
=> DBGInvoke mock_schema_syncer('true')
┌─mock_schema_syncer("true")─┐
│ mock schema syncer enabled │
└────────────────────────────┘

=> DBGInvoke __mock_tidb_table(default, test, 'col_1 String, col_2 Int32', col_2)
=> DBGInvoke __put_region(4, 0, 100, default, test)

=> DBGInvoke __raft_insert_row(default, test, 4, 51, 'test1', 51)
=> DBGInvoke __raft_insert_row(default, test, 4, 52, 'test2', 52)
=> DBGInvoke __raft_insert_row(default, test, 4, 53, 'test3', 53)
=> DBGInvoke __raft_insert_row(default, test, 4, 54, 'test3', 54)

=> selraw col_1, col_2, _INTERNAL_DELMARK from default.test
┌─col_1─┬─col_2─┬─_INTERNAL_DELMARK─┐
│ test2 │    52 │                 0 │
└───────┴───────┴───────────────────┘
┌─col_1─┬─col_2─┬─_INTERNAL_DELMARK─┐
│ test3 │    53 │                 0 │
└───────┴───────┴───────────────────┘
┌─col_1─┬─col_2─┬─_INTERNAL_DELMARK─┐
│ test3 │    54 │                 0 │
└───────┴───────┴───────────────────┘
┌─col_1─┬─col_2─┬─_INTERNAL_DELMARK─┐
│ test1 │    51 │                 0 │
└───────┴───────┴───────────────────┘

=> selraw count(*) from default.test
┌─count()─┐
│       4 │
└─────────┘

=> selraw avg(col_2), col_1 from default.test where col_1 > 'test1' group by col_1
┌─avg(col_2)─┬─col_1─┐
│       53.5 │ test3 │
│         52 │ test2 │
└────────────┴───────┘

=> drop table if exists default.test
=> DBGInvoke __drop_tidb_table(default, test)
