=> drop table if exists default.test
=> DBGInvoke __drop_tidb_table(default, test)

=> DBGInvoke __set_flush_threshold(100000, 0)
=> DBGInvoke mock_schema_syncer('true')
┌─mock_schema_syncer("true")─┐
│ mock schema syncer enabled │
└────────────────────────────┘

=> DBGInvoke __mock_tidb_table(default, test, 'col_1 String, col_2 Int32', col_2)
=> DBGInvoke __put_region(0, 0, 100, default, test)

=> DBGInvoke __raft_insert_row(default, test, 0, 1, 'test1', 1)
=> DBGInvoke __raft_insert_row(default, test, 0, 2, 'test2', 2)
=> DBGInvoke __raft_insert_row(default, test, 0, 3, 'test3', 3)
=> DBGInvoke __raft_insert_row(default, test, 0, 4, 'test3', 4)

=> select * from default.test
┌─col_1─┬─col_2─┐
│ test1 │     1 │
│ test2 │     2 │
│ test3 │     3 │ 
│ test3 │     4 │ 
└───────┴───────┘

=> select count(*) from default.test
┌─count()─┐
│       4 │
└─────────┘

=> select avg(col_2) from default.test
┌─avg(col_2)─┐
│        2.5 │
└────────────┘

=> select avg(col_2), col_1 from default.test where col_1 > 'test1' group by col_1
┌─avg(col_2)─┬─col_1─┐
│        3.5 │ test3 │
│          2 │ test2 │
└────────────┴───────┘

=> drop table if exists default.test
=> DBGInvoke __drop_tidb_table(default, test)
