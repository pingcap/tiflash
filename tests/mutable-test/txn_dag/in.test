# Preparation.
=> DBGInvoke __enable_schema_sync_service('true')

=> DBGInvoke __drop_tidb_table(default, test)
=> drop table if exists default.test

=> DBGInvoke __set_flush_threshold(1000000, 1000000)

# Data.
=> DBGInvoke __mock_tidb_table(default, test, 'col_1 Int64, col_2 Int64')
=> DBGInvoke __refresh_schemas()
=> DBGInvoke __put_region(4, 0, 100, default, test)
=> DBGInvoke __raft_insert_row(default, test, 4, 50, 666, 666)
=> DBGInvoke __raft_insert_row(default, test, 4, 51, 776, 777)
=> DBGInvoke __raft_insert_row(default, test, 4, 52, 777, 777)

=> DBGInvoke dag('select * from default.test where col_1 in (777,888)')
┌─col_1─┬─col_2─┐
│   777 │   777 │
└───────┴───────┘

=> DBGInvoke dag('select * from default.test where col_1 in (col_2,777,888)')
┌─col_1─┬─col_2─┐
│   666 │   666 │
│   777 │   777 │
└───────┴───────┘

=> DBGInvoke dag('select * from default.test where col_1 not in (777,888)')
┌─col_1─┬─col_2─┐
│   666 │   666 │
│   776 │   777 │
└───────┴───────┘

=> DBGInvoke dag('select * from default.test where col_1 not in (col_2,777,888)')
┌─col_1─┬─col_2─┐
│   776 │   777 │
└───────┴───────┘

# Clean up.
=> DBGInvoke __drop_tidb_table(default, test)
=> drop table if exists default.test
