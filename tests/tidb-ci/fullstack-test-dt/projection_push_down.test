mysql> use test;
mysql> drop table if exists t;
mysql> create table t (id int, value decimal(6,3));
mysql> alter table t set tiflash replica 1;
mysql> insert into t values(1,2.3),(3,4.5),(5,6.7);

func> wait_table test t

mysql> use test; set session tidb_isolation_read_engines='tiflash'; select id+1, value+1 from t order by id;
+------+---------+
| id+1 | value+1 |
+------+---------+
|    2 |   3.300 |
|    4 |   5.500 |
|    6 |   7.700 |
+------+---------+

mysql> insert into t values(null,null),(1,null),(null,3.3);

func> wait_table test t

mysql> use test; set session tidb_isolation_read_engines='tiflash'; select id+1, value+1 from t order by id;
+------+---------+
| id+1 | value+1 |
+------+---------+
| NULL |    NULL |
| NULL |   4.300 |
|    2 |   3.300 |
|    2 |    NULL |
|    4 |   5.500 |
|    6 |   7.700 |
+------+---------+
