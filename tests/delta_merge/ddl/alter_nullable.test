>> drop table if exists dm_test

>> create table dm_test (
        a Int16,
        b Nullable(Int32),
        c Nullable(Int32),
        d Nullable(String),
        e Nullable(DateTime)
   ) engine = DeltaMerge(a)

>> insert into table dm_test(a) values(100)
>> insert into table dm_test values(20000, 2048, 65535, 'Hello TiFlash', '1999-09-09 00:00:00')
>> select * from dm_test
